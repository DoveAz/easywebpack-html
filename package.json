{
  "name": "easywebpack-html",
  "version": "1.0.0",
  "description": "static html build solution for easywebpack",
  "keywords": [
    "webpack",
    "easywebpack",
    "html"
  ],
  "dependencies": {
    "easywebpack": "^3.3.8"
  },
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.0.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.6.1",
    "eslint-config-eslint": "^4.0.0",
    "mocha": "^3.4.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_SERVER=true NODE_ENV=development node test/build",
    "build": "cross-env NODE_ENV=development node test/build",
    "easywebpack:start": "cross-env easywebpack server -f test/build/webpack.config.js",
    "easywebpack:build": "cross-env easywebpack build -f test/build/webpack.config.js",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "test": "mocha test",
    "ci": "npm run lint && npm run cov"
  },
  "files": [
    "index.js",
    "lib",
    "config",
    "tool"
  ],
  "ci": {
    "version": "6, 7, 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubcarl/easywebpack-html.git"
  },
  "bugs": {
    "url": "https://github.com/hubcarl/easywebpack-html/issues"
  },
  "homepage": "https://github.com/hubcarl/easywebpack-html#readme",
  "author": "hubcarl@126.com",
  "license": "MIT"
}
